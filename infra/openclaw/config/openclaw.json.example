{
  "$schema": "https://openclaw.ai/schema/v2.json",
  "gateway": {
    "mode": "local",
    "auth": {
      "mode": "token",
      "token": "your-gateway-token-here"
    },
    "trustedProxies": ["172.18.0.1", "127.0.0.1"]
  },
  "agents": {
    "defaults": {
      "workspace": "/workspace",
      "sandbox": {
        "mode": "non-main"
      },
      "model": "openrouter/stepfun/step-3.5-flash:free"
    },
    "list": [
      {
        "id": "daatan",
        "default": true,
        "name": "Corvus",
        "workspace": "/workspace/daatan",
        "identity": {
          "name": "Corvus",
          "theme": "DAATAN forecasting platform",
          "emoji": "ðŸ”®"
        },
        "skills": [
          "docker",
          "terraform",
          "github-actions",
          "nextjs",
          "daatan-deploy"
        ],
        "model": "openrouter/upstage/solar-pro-3:free"
      },
      {
        "id": "calendar",
        "name": "Calendar Agent",
        "workspace": "/workspace/year-shape",
        "identity": {
          "name": "Calendar Agent",
          "theme": "YearWheel (Vite, TypeScript, Tailwind)",
          "emoji": "ðŸ“…"
        },
        "skills": [
          "vite",
          "typescript",
          "cloudflare-pages",
          "tailwind"
        ],
        "model": "openrouter/upstage/solar-pro-3:free"
      }
    ]
  },
  "bindings": [
    {
      "agentId": "daatan",
      "match": {
        "channel": "telegram",
        "accountId": "daatan"
      }
    },
    {
      "agentId": "calendar",
      "match": {
        "channel": "telegram",
        "accountId": "calendar"
      }
    }
  ],
  "models": {
    "providers": {
      "openrouter": {
        "apiKey": "${OPENROUTER_API_KEY}",
        "baseUrl": "https://openrouter.ai/api/v1",
        "api": "openai-completions",
        "models": [
          {
            "id": "upstage/solar-pro-3:free",
            "name": "Solar Pro 3 (free)"
          },
          {
            "id": "stepfun/step-3.5-flash:free",
            "name": "Step 3.5 Flash (free)"
          },
          {
            "id": "z-ai/glm-4.5-air:free",
            "name": "GLM 4.5 Air (free)"
          }
        ]
      },
      "ollama": {
        "baseUrl": "http://host.docker.internal:11434/v1",
        "apiKey": "ollama-local",
        "api": "openai-responses",
        "models": [
          {
            "id": "qwen2.5:1.5b",
            "name": "Qwen 2.5 1.5B"
          }
        ]
      }
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "pairing",
      "streamMode": "partial",
      "accounts": {
        "daatan": {
          "name": "Daatan",
          "botToken": "${TELEGRAM_BOT_TOKEN_DAATAN}"
        },
        "calendar": {
          "name": "Calendar",
          "botToken": "${TELEGRAM_BOT_TOKEN_CALENDAR}"
        }
      }
    }
  },
  "browser": {
    "enabled": true,
    "headless": true
  },
  "plugins": {
    "entries": {
      "telegram": {
        "enabled": true
      }
    }
  },
  "auth": {
    "profiles": {
      "openrouter:default": {
        "provider": "openrouter",
        "mode": "api_key"
      }
    }
  }
}
