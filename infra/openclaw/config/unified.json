{
  "$schema": "https://openclaw.ai/schema/v2.json",
  "version": "2026.2",
  "meta": {
    "description": "OpenClaw unified gateway: Daatan + Calendar agents, two Telegram bots",
    "deployment": "EC2 t4g.medium",
    "updated": "2026-02-18",
    "budget": "$5/month OpenRouter"
  },
  "gateway": {
    "bind": "0.0.0.0:18789",
    "auth": {
      "mode": "password"
    },
    "tailscale": {
      "mode": "off"
    }
  },
  "agents": {
    "defaults": {
      "workspace": "/workspace",
      "sandbox": {
        "mode": "non-main"
      },
      "model": {
        "primary": "google/gemini-1.5-pro",
        "fallback": "openrouter/qwen/qwen-2.5-7b-instruct",
        "localFallback": "ollama/qwen2.5:1.5b"
      }
    },
    "list": [
      {
        "id": "daatan",
        "default": true,
        "name": "Corvus",
        "workspace": "/workspace/daatan",
        "identity": {
          "name": "Corvus",
          "theme": "DAATAN forecasting platform (Next.js, Prisma, TypeScript)",
          "emoji": "ðŸ”®"
        },
        "skills": {
          "managed": [
            "docker",
            "terraform",
            "github-actions",
            "nextjs"
          ],
          "workspace": [
            "daatan-deploy"
          ]
        },
        "model": {
          "primary": "google/gemini-1.5-pro",
          "fallback": "openrouter/qwen/qwen-2.5-7b-instruct",
          "localFallback": "ollama/qwen2.5:1.5b"
        }
      },
      {
        "id": "calendar",
        "name": "Calendar Agent",
        "workspace": "/workspace/year-shape",
        "identity": {
          "name": "Calendar Agent",
          "theme": "YearWheel (Vite, TypeScript, Tailwind)",
          "emoji": "ðŸ“…"
        },
        "skills": {
          "managed": [
            "vite",
            "typescript",
            "cloudflare-pages",
            "tailwind"
          ]
        },
        "model": {
          "primary": "google/gemini-1.5-pro",
          "fallback": "openrouter/qwen/qwen-2.5-7b-instruct",
          "localFallback": "ollama/qwen2.5:1.5b"
        }
      }
    ]
  },
  "bindings": [
    {
      "agentId": "daatan",
      "match": {
        "channel": "telegram",
        "accountId": "daatan"
      }
    },
    {
      "agentId": "calendar",
      "match": {
        "channel": "telegram",
        "accountId": "calendar"
      }
    }
  ],
  "models": {
    "providers": {
      "google": {
        "apiKey": "${GEMINI_API_KEY}"
      },
      "openrouter": {
        "apiKey": "${OPENROUTER_API_KEY}",
        "baseUrl": "https://openrouter.ai/api/v1",
        "headers": {
          "HTTP-Referer": "https://github.com/komapc/daatan",
          "X-Title": "DAATAN"
        }
      },
      "ollama": {
        "baseUrl": "http://host.docker.internal:11434/v1",
        "apiKey": "ollama-local",
        "api": "openai-responses",
        "autoDiscover": true,
        "models": [
          {
            "id": "qwen2.5:1.5b",
            "name": "Qwen 2.5 1.5B",
            "contextWindow": 32768,
            "maxOutput": 2048,
            "useFor": [
              "qa",
              "health-checks",
              "simple-tasks"
            ]
          },
          {
            "id": "qwen2.5:3b",
            "name": "Qwen 2.5 3B",
            "contextWindow": 32768,
            "maxOutput": 4096,
            "useFor": [
              "fallback",
              "code-assistance"
            ]
          }
        ]
      }
    },
    "routing": {
      "primary": "google/gemini-1.5-pro",
      "fallback": "openrouter/qwen/qwen-2.5-7b-instruct",
      "localFallback": "ollama/qwen2.5:1.5b"
    },
    "budget": {
      "openrouter": {
        "monthlyLimit": 5.00,
        "dailyLimit": 0.20,
        "alertAt": 0.80
      }
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "pairing",
      "streamMode": "partial",
      "chatId": "${TELEGRAM_CHAT_ID}",
      "accounts": {
        "daatan": {
          "name": "Daatan",
          "botToken": "${TELEGRAM_BOT_TOKEN_DAATAN}"
        },
        "calendar": {
          "name": "Calendar",
          "botToken": "${TELEGRAM_BOT_TOKEN_CALENDAR}"
        }
      }
    }
  },
  "browser": {
    "enabled": true,
    "headless": true,
    "maxConcurrent": 2
  },
  "security": {
    "sandbox": {
      "enabled": true,
      "mode": "non-main",
      "dockerSocket": "/var/run/docker.sock"
    }
  },
  "plugins": {
    "entries": {
      "telegram": {
        "enabled": true
      }
    }
  },
  "auth": {
    "profiles": {
      "google:default": {
        "provider": "google",
        "mode": "api_key"
      },
      "openrouter:default": {
        "provider": "openrouter",
        "mode": "api_key"
      }
    }
  }
}